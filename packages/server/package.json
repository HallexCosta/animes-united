{
  "extends": "../../babel.config.js",
  "name": "@animes-united/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir ../../dist/server --copy-files --no-copy-ignored",
    "start:development": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "start:production": "node ../../dist/server/src/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/mongodb": "^3.6.3",
    "@types/puppeteer": "^5.4.2",
    "@types/supertest": "^2.0.10",
    "supertest": "^6.0.1",
    "ts-node-dev": "^1.0.0"
  },
  "private": true
}
