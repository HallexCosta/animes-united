{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "all": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "include": [
    "src/**/*.ts"
  ],
  "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
  ],
  "report-dir": "coverage"
}
